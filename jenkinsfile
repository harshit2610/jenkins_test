pipeline {
  agent any
  stages {
    stage('Build and push Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonacci:2.0", "-f Dockerfile .")
          docker.withRegistry('https://registry.hub.docker.com', 'docker_har') {
            builtImage.push("harshit2610/fibonnaci:2.0")
          }
        }
      }
    }
  }
}
