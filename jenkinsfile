pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonnaci:2.0", "-f Dockerfile .")
          env.IMAGE_TAG = "${builtImage.imageId}"
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker_har') {
            docker.image(env.IMAGE_TAG).push("harshit2610/fibonnaci:2.0")
          }
        }
      }
    }
  }
}

