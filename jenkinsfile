pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonnaci:2.0", "-f Dockerfile .")
         def taggedImage = builtImage.tag("harshit2610/fibonnaci:2.0")
          docker.withRegistry('https://registry.hub.docker.com', 'docker_har') {
            taggedImage.push("harshit2610/fibonnaci:2.0")
          }
        }
      }
    }
  }
}

