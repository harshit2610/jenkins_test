pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
         script {
                    def dockerImage = docker.build("fibonnaci:2.0", "-f Dockerfile .")
                    docker.withRegistry('https://registry.hub.docker.com', 'docker_har') {
                        dockerImage.push("harshit2610/fibonnaci:2.0")
                       
                    }
        }
      }
    }
  }
}

