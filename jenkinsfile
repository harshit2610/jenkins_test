pipeline {
  agent any
  stages {
    stage('Build and push Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonacci:2.0", "-f Dockerfile .")
          docker.withRegistry('https://hub.docker.com/repository/docker/harshit2610/fibonnaci/general', 'docker_har') {

            builtImage.push()
          }
        }
      }
    }
  }
}
