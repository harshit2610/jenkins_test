pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
          script {
          def builtImage = docker.build("fibonnaci:2.0", "-f Dockerfile .")
          
          def taggedImage = builtImage.tag("2.0")
          
          docker.withRegistry('https://index.docker.io/v1/', 'docker_har') {
            taggedImage.push()
          }
      }
    }
  }
}

