pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          docker.build('harshit2610/fibonacci:2.0')
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker_harshit') {
            docker.image('harshit2610/fibonacci:2.0').push()
          }
        }
      }
    }
  }
}
