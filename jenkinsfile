pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonacci:2.0", "-f Dockerfile .")
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://index.docker.io/v1/', 'docker_harshit') {
            builtImage.push()
          }
        }
      }
    }
  }
}
