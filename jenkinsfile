pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
          script {
          def dockerImage = docker.build("fibonnaci:2.0", "-f Dockerfile .")
          docker.withRegistry('https://index.docker.io/v1/', 'docker_har') {
          dockerImage.tag("2.0", "harshit2610/fibonnaci:2.0")
          dockerImage.push("2.0")          
}
      }
    }
  }
}
}
